{
  "name": "ya-vue-blog",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Micheal Gardner <twocucao@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:new": "webpack-dashboard -- webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:theme": "cd ./packages/theme-future && watchmedo shell-command -p '*.scss' -c 'npm run build:theme && mkdir -p ../../src/assets/theme-future/ && cp -r ../../packages/theme-future/lib/* ../../src/assets/theme-future/ && mkdir -p ../../src/assets/theme-future-scss/ && cp -r ../../packages/theme-future/src/* ../../src/assets/theme-future-scss' -R -D .",
    "dev:iconfont": "watchmedo shell-command -p './extra/svg-icon/*.svg' -c 'npm run build:iconfont' -R -D .",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build:iconfont": "node build/build_iconfont.js",
    "build:theme": "cd ./packages/theme-future/ && npm run build && cd - && mkdir -p ./src/assets/theme-future/ && cp -r ./packages/theme-future/lib/* ./src/assets/theme-future/",
    "build": "node build/build.js"
  },
